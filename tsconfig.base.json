{
  "compilerOptions": {
    "composite": true,
    "declarationMap": true,
    "importHelpers": true,
    "isolatedModules": true,
    "lib": ["es2022", "DOM"],
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "noEmitOnError": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitOverride": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "es2022",
    "customConditions": ["@nx-sandbox/source"],
    "baseUrl": ".",
    "paths": {
      "@mobile/*": ["apps/mobile/src/*"],
      "@api/*": ["apps/api/src/*"],
      "@nx-sandbox/ui": ["packages/ui/src/index.ts"],
      "webOne/Routes": ["apps/webOne/src/app/remote-entry/entry.routes.ts"],
      "webTwo/Routes": ["apps/webTwo/src/app/remote-entry/entry.routes.ts"],
      "@nx-sandbox/theme": ["packages/theme/src/index.ts"]
    }
  }
}
